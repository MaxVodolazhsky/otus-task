<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="studyTestReader" class="ru.vodolazhsky.otus.reader.StudyTestReaderCsv">
        <property name="questions" value="classpath:/data/question.csv"/>
    </bean>
    <bean id="studyTestService" class="ru.vodolazhsky.otus.service.ShowTestServiceImpl" autowire="constructor">
        <constructor-arg name="studyTestReader" ref="studyTestReader"/>
    </bean>
</beans>